@{
    ViewData["Title"] = "Editar contato";
}
<div class="text-center">

    <h1 class="display-4">Editar Contato</h1>

</div>

<form>
    <div class="form-group">
        <label for="nome">Nome</label>
        <p>
            <input type="text" class="form-control" placeholder="Nome">

    </div>

    <div class="form-group">
        <label for="E-mail">Email</label>
        <p>
            <input type="email" class="form-control" placeholder="E-mail">

    </div>

    <div class="form-group">
        <label for="Celular">Celular</label>
        <p>
            <input type="text" class="form-control" placeholder="Celular">
    </div>


    <button type="submit" class="btn btn-primary w-100">Alterar</button>
    <p></p>
    <a class="btn btn-secondary w-100" asp-controler="contato" asp-action="Index" role="button">Voltar</a>
</form>
